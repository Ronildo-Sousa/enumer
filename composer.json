{
    "name": "ronildo-sousa/enumer",
    "description": "A simple package to create PHP enum from a json file",
    "type": "library",
    "keywords": [
        "php",
        "enum",
        "json",
        "conversion"
      ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RonildoSousa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RonildoSousa\\tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "authors": [
        {
            "name": "Ronildo Sousa",
            "email": "sousaronildo400@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
